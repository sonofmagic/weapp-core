{
  "name": "weapp-fetch",
  "version": "1.0.0",
  "private": true,
  "description": "Thin fetch wrapper built on top of @weapp-core/http for mini programs",
  "scripts": {
    "build": "unbuild",
    "test": "vitest run",
    "test:dev": "vitest",
    "release": "pnpm publish"
  },
  "keywords": [
    "weapp",
    "wechat",
    "mini-program",
    "fetch"
  ],
  "author": "SonOfMagic <qq1324318532@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/sonofmagic/weapp-core/tree/main/packages/fetch#readme",
  "bugs": {
    "url": "https://github.com/sonofmagic/weapp-core/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sonofmagic/weapp-core.git",
    "directory": "packages/fetch"
  },
  "sideEffects": false,
  "type": "commonjs",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "dependencies": {
    "@weapp-core/http": "workspace:^"
  }
}
